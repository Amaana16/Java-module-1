import java.util.*;
class square {
    public static void main(String[] args) {
        int n[]=new int[10];
         n[0]=1;
         n[1]=4;
         n[2]=6;
         n[3]=8;
         n[4]=10;
         n[5]=12;
         n[6]=14;
         n[7]=16;
         n[8]=18;
         n[9]=20;
         System.out.println(n[5]);
    }
}
